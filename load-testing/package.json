  "description": "Utility tools for running load tests on Electric",
  "main": "main.js",
  "scripts": {
    "load-test": "node main.js",
    "docker-compose:up": "docker-compose -- -f $COMPOSE_FILE --env-file $ENV_FILE up -d",
